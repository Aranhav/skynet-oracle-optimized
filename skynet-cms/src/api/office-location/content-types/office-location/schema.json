{
  "kind": "collectionType",
  "collectionName": "office_locations",
  "info": {
    "singularName": "office-location",
    "pluralName": "office-locations",
    "displayName": "Office Location"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "workingHours": {
      "type": "string"
    },
    "mapUrl": {
      "type": "text"
    },
    "isHeadOffice": {
      "type": "boolean"
    },
    "addresses": {
      "type": "component",
      "repeatable": true,
      "component": "address.addresses"
    },
    "coordinates": {
      "type": "component",
      "repeatable": true,
      "component": "coordinates.coordinates"
    }
  }
}
