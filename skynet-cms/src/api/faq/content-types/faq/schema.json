{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "FAQ"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "answer": {
      "type": "blocks"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "General",
        "Shipping",
        "Tracking",
        "Pricing",
        "Support"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
